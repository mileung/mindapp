{
	"manifest_version": 3,
	"name": "Mindapp",
	"version": "1.0.1",
	"icons": { "1024": "src/mindapp-logo.png" },
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
	},
	"content_scripts": [
		{
			"js": ["src/contentScript.ts"],
			"matches": ["<all_urls>"]
		}
	]
}
